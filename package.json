    "babel-preset-vue-app": "^2.0.0",
    "css-loader": "^1.0.1",
    "element-theme-chalk": "^2.4.11",
    "eslint": "^5.9.0",
    "eslint-plugin-vue": "^5.0.0-beta.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
